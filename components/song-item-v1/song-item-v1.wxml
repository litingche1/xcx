<!--components/song-item-v1/song-item-v1.wxml-->
<view class="song-item" bindtap="onSongItemTap">
  <image src="{{itemData.al.picUrl}}" class="image" />
  <view class="content">
    <view class="name">
      {{itemData.name}}
    </view>
    <view class="source">
      {{itemData.ar[0].name}}
    </view>
  </view>
  <view class="arrow">
    <van-icon name="arrow" />
  </view>
</view>